{
  "name": "michaelhsu",
  "version": "0.1.0",
  "description": "The Michael Hsu CLI",
  "repository": "https://github.com/evenchange4/michaelhsu",
  "author": "Michael Hsu <evenchange4@gmail.com>",
  "license": "MIT",
  "files": ["src"],
  "main": "src/cli.js",
  "bin": {
    "michaelhsu": "src/cli.js"
  },
  "scripts": {
    "eslint": "eslint ./",
    "flow": "flow",
    "format":
      "prettier --write 'src/**/*.{js,json}' 'bin/**/*.{js,json}' 'tests/**/*.{js,json}' '*.md' '*.json'"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "date-fns": "^1.29.0",
    "graphql-request": "^1.6.0",
    "import-jsx": "^1.3.0",
    "ink": "^0.4.1",
    "ink-select-input": "^1.1.0",
    "opn": "^5.3.0",
    "term-img": "^2.1.0",
    "terminal-image": "^0.1.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "codecov": "^3.0.2",
    "danger": "^3.7.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.72.0",
    "github-changes": "^1.1.2",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "lint-staged": "^7.1.1",
    "prettier": "^1.12.1"
  },
  "lint-staged": {
    "*.{js,json,md}": ["prettier --write", "git add"]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base",
      "plugin:flowtype/recommended",
      "plugin:jest/recommended",
      "prettier",
      "prettier/react",
      "prettier/flowtype"
    ],
    "plugins": ["prettier", "jest", "flowtype"],
    "env": {
      "jest/globals": true
    },
    "rules": {
      "prettier/prettier": "error"
    }
  }
}
